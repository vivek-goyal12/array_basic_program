package practice;

public class ArrayDemo {
    public static void main(String[] args) {
        int squares[]= new int[6];
        for(int i =0;i<5;i++){
            int n = i+1;
            int x = n*n;
             squares[i] = x;
        }

            for(int i =0;i<5;i++) {

            System.out.println(squares[i]+" ");
        }
    }
}
